@model myRemiza.Models.Strazacy

@{
    ViewData["Title"] = "Delete";
}

<h1>Usuń</h1>

<h3>Jesteś pewien, że chcesz usnąć tego strażaka</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Imie)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Imie)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nazwisko)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nazwisko)
        </dd>
        <dt class = "col-sm-2">
           Data urodzenia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data_urodzenia)
        </dd>
        <dt class = "col-sm-2">
            Data wstąpienia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data_wstapienia)
        </dd>
        <dt class = "col-sm-2">
            Zaliczone kursy
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kursy)
        </dd>
        <dt class = "col-sm-2">
           Odznaczenia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Odznaczenia)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Usuń" class="btn btn-danger" /> |
        <a asp-action="Index">Powrót</a>
    </form>
</div>
